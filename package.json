{
	"name": "language-file-magic",
	"version": "1.0.1",
	"description": "Syntax highlighting for file(1) magic data.",
	"keywords": ["File", "Magic", "MIME", "Magic number", "Signature", "Header"],
	"repository": "https://github.com/Alhadis/language-file-magic",
	"license": "ISC",
	"engines": {
		"atom": "*"
	},
	"devDependencies": {
		"@alhadis/eslint-config": "^2.4.1",
		"eslint": "^8.22"
	},
	"eslintConfig": {
		"extends": "@alhadis/eslint-config/atom"
	},
	"configSchema": {
		"paths": {
			"type": "array",
			"default": ["magic/Magdir/*", "file/magic/*"],
			"items": {"type": "string"}
		}
	}
}
